{
  "name": "authsite-server",
  "version": "1.0.0",
  "description": "Authentication website using Typescript, Express, Mongo, Passport and JSX as view engine",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run client:build && npm install tsc && node dist/server.js",
    "dev": "tsc && node dist/server.js",
    "watch": "nodemon --watch src/** --ext ts,tsx --exec \"npm dev\"",
    "client:build": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danlopes-arc/authsite-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danlopes-arc/authsite-server/issues"
  },
  "homepage": "https://github.com/danlopes-arc/authsite-server#readme",
  "dependencies": {
    "@types/passport-github": "^1.1.5",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.15",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "typescript": "^4.1.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.9",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/validate.js": "^0.11.0",
    "nodemon": "^2.0.6",
    "tslint": "^6.1.3"
  }
}
